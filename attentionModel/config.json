{
  "dataset_path": "../data",
  "dataset_train": "davidson_html_train.tsv",
  "dataset_test": "davidson_html_test.tsv",

  "num_epochs": 20,
  "batch_size": 32,
  "learning_rate": 0.0003,
  "balanced_weights": false,
  "save_model": true,
  "embeddings": "elmo",
